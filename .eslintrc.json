{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:vue/essential",
    "airbnb-base",
    "airbnb-typescript/base",
    "prettier"
  ],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "parser": "@typescript-eslint/parser",
    "project": "./tsconfig.json",
    "sourceType": "module"
  },
  "rules": {
    "vue/no-multiple-template-root": "off"
  }
}
